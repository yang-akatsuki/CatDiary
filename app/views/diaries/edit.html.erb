<div class="newContent blueBackground">
  <div id="log" class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="contents">
          <%= form_tag("/diaries/#{@diary.id}", method: :patch ) do %>
          <h3>
          編集する
          </h3>
          <input placeholder="Image Url" type="text" name="image" value="<%= @diary.image %>" autofocus="true">
          <textarea cols="30" name="text" placeholder="text" rows="10"><%= @diary.text %></textarea>
          <input type="submit" value="SENT">
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>